{
  "name": "shell-factory-cocos",
  "version": "1.0.0",
  "description": "Common commands collection for shell-factory",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "test": "npx tsc -p tsconfig-tests.json && npx c8 -r html mocha ./tests-build/tests/*.test.js",
    "testCleanup": "rm -rf ./tests-build",
    "testAndCleanup": "npm run test; npm run testCleanup",
    "lint": "npx eslint .",
    "generateDocs": "npx typedoc"
  },
  "author": "monstermichl",
  "license": "MIT",
  "dependencies": {
    "shell-factory": "3.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "eslint": "^8.34.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.27",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "bourne",
    "shell",
    "generator",
    "builder",
    "typescript",
    "dynamically",
    "chainable",
    "common",
    "commands"
  ]
}
